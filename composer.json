{
  "name": "chamber-orchestra/meta-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle providing SEO meta data entity traits, forms, and DTOs",
  "keywords": [
    "symfony",
    "seo",
    "meta",
    "doctrine",
    "bundle"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Andrew Lukin",
      "email": "lukin.andrej@gmail.com",
      "homepage": "https://github.com/wtorsi",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.5",
    "doctrine/orm": "^3.0",
    "symfony/http-foundation": "8.0.*",
    "symfony/http-kernel": "8.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^13.0"
  },
  "suggest": {},
  "autoload": {
    "psr-4": {
      "ChamberOrchestra\\MetaBundle\\": ""
    },
    "exclude-from-classmap": [
      "/tests/",
      "/Cms/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
